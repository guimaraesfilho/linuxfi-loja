<html>
  <head>
    <title>Loja <%= @titulo %>    </title>
    
  </head>
  <body>
    
    <h1> <%= yield :cabecalho %> | loja </h1>

    <% unless flash.blank? %>
      <% flash.each do |key,value| %>
        <p class="flash_<%= key %>"> <%= value %> </p>
      <%end%>
    <% end %>

    <%= yield %>
  </body>
</html>
